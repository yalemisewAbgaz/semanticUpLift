    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.localhost:3030/OLDCA#hasAnswer -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#hasAnswer">
        <owl:inverseOf rdf:resource="http://www.localhost:3030/OLDCA#isAnswerOf"/>
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Answer"/>
        <rdfs:comment xml:lang="en">The hasAnswer property relates the question to the answer of the question.</rdfs:comment>
        <rdfs:label xml:lang="en">has Answer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasAuthor -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#hasAuthor">
        <owl:inverseOf rdf:resource="http://www.localhost:3030/OLDCA#isAuthorOf"/>
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Author"/>
        <rdfs:comment xml:lang="en">The hasAuthor property relates the questionnaire to one or more author of the questionnaire.</rdfs:comment>
        <rdfs:label xml:lang="en">has Author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasQuestion -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#hasQuestion">
        <owl:inverseOf rdf:resource="http://www.localhost:3030/OLDCA#isQuestionOf"/>
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">The hasQuestion property relates the questionnaire to one or more question which is contained in it.</rdfs:comment>
        <rdfs:label xml:lang="en">has Question</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasRelatedQuestionnaire -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#hasRelatedQuestionnaire">
        <owl:inverseOf rdf:resource="http://www.localhost:3030/OLDCA#isRelatedQuestionnaireOf"/>
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:comment xml:lang="en">The hasRelatedQuestionnaire property relates the questionnaire to one or more related questionnaire that share similar topic.</rdfs:comment>
        <rdfs:label xml:lang="en">has related Questionnaire</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasSubQuestion -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#hasSubQuestion">
        <owl:inverseOf rdf:resource="http://www.localhost:3030/OLDCA#isSubQuestionOf"/>
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">The hasSubQuestion property relates the question to one or more subquestions under it.</rdfs:comment>
        <rdfs:label xml:lang="en">has subQuestion</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasTopic -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#hasTopic">
        <owl:inverseOf rdf:resource="http://www.localhost:3030/OLDCA#isTopicOf"/>
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Topic"/>
        <rdfs:comment xml:lang="en">The hasTopic property relates the questionnaire or the question to the topic of the questionnaire.</rdfs:comment>
        <rdfs:label xml:lang="en">has Topic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#isAnswerOf -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#isAnswerOf">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Answer"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">The isAnswerOf property relates the Answer to the question.</rdfs:comment>
        <rdfs:label xml:lang="en">is Answer of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#isAuthorOf -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#isAuthorOf">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Author"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:comment xml:lang="en">The isAuthorOf property relates the Author to the questionnaire.</rdfs:comment>
        <rdfs:label xml:lang="en">is Author of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#isQuestionOf -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#isQuestionOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:comment xml:lang="en">The isQuestionOf property relates the question to one questionnaire.</rdfs:comment>
        <rdfs:label xml:lang="en">is Question of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#isRelatedQuestionnaireOf -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#isRelatedQuestionnaireOf">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:comment xml:lang="en">The hasRelatedQuestionnaire property relates the questionnaire to one or more related questionnaire that share similar topic.</rdfs:comment>
        <rdfs:label xml:lang="en">is related Questionnaire of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#isSubQuestionOf -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#isSubQuestionOf">
        <rdfs:comment xml:lang="en">The isSubQuestionOf property relates a subquestion to one or more questions.</rdfs:comment>
        <rdfs:label xml:lang="en">is SubQuestion of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.localhost:3030/OLDCA#isTopicOf -->

    <owl:ObjectProperty rdf:about="http://www.localhost:3030/OLDCA#isTopicOf">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Topic"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:range rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:comment xml:lang="en">The isTopicOf property relates the topic with either the questionnaire or the question it is discussed in.</rdfs:comment>
        <rdfs:label xml:lang="en">is Topic of</rdfs:label>
    </owl:ObjectProperty>
    


