    <!-- http://www.localhost:3030/OLDCA#hasFirstName -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasFirstName">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Author"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">the first name of a person.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <rdfs:label xml:lang="en">firstName</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasKeywords -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasKeywords">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">Keywords that represent the topic of the questionnaire.</rdfs:comment>
        <rdfs:label xml:lang="en">Keywords</rdfs:label>
        <rdfs:label xml:lang="de">Schlagwoerter</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasLastName -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasLastName">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Author"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The last name of a person.</rdfs:comment>
        <rdfs:label xml:lang="en">lastName</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasMiddleName -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasMiddleName">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Author"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The middle name of a person</rdfs:comment>
        <rdfs:label xml:lang="en">middleName</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasNote -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasNote">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">A short note associated with the questionnaire.</rdfs:comment>
        <rdfs:label xml:lang="de">Anmerkung</rdfs:label>
        <rdfs:label xml:lang="en">Note</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasNumber -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasNumber">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The unique number assigned to the question or the questionnaire</rdfs:comment>
        <rdfs:label xml:lang="en">Number</rdfs:label>
        <rdfs:label xml:lang="de">nummer</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasOriginalData -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasOriginalData">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The original Question printed on the questionnaire and used during data collection.</rdfs:comment>
        <rdfs:label xml:lang="en">Original Data</rdfs:label>
        <rdfs:label xml:lang="de">originaldaten</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasOriginalQuestion -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasOriginalQuestion">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The original Question asked</rdfs:comment>
        <rdfs:label xml:lang="en">Original Question</rdfs:label>
        <rdfs:label xml:lang="de">originalfrage</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasReleaseDate -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasReleaseDate">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">The release date of the questionnaire. The date the questionnaire is released for collection of data?</rdfs:comment>
        <rdfs:label xml:lang="de">Freigabe</rdfs:label>
        <rdfs:label xml:lang="en">Release Date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasShortQuestion -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasShortQuestion">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The short version of the original Question asked.</rdfs:comment>
        <rdfs:label xml:lang="de">Kurzfrage</rdfs:label>
        <rdfs:label xml:lang="en">Short Question</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasTitle -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasTitle">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The title of the questionnaire.</rdfs:comment>
        <rdfs:label xml:lang="de">Titel</rdfs:label>
        <rdfs:label xml:lang="en">Title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#hasYearOfPublication -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#hasYearOfPublication">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">The year in which the questionnaire is published.</rdfs:comment>
        <rdfs:label xml:lang="de">Erscheinungsjahr</rdfs:label>
        <rdfs:label xml:lang="en">Year of Publication</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.localhost:3030/OLDCA#isChecked -->

    <owl:DatatypeProperty rdf:about="http://www.localhost:3030/OLDCA#isChecked">
        <rdfs:domain rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">indicates the questionnaire is checked?</rdfs:comment>
        <rdfs:label xml:lang="de">Checked</rdfs:label>
        <rdfs:label xml:lang="en">Checked</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.localhost:3030/OLDCA#AdditionalQuestionnaire -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#AdditionalQuestionnaire">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:comment xml:lang="en">Additional Questionnaire is a Questionnaire that is used as a supplementary questionnaire to the systematic questionnaire.</rdfs:comment>
        <rdfs:label xml:lang="en">Additional Questionnaire</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#Answer -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#Answer">
        <rdfs:comment xml:lang="en">An Answer represents a written, spoken or illustrated response to a question.</rdfs:comment>
        <rdfs:label xml:lang="en">Answer</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#Author -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#Author">
        <rdfs:comment xml:lang="en">Authors/ collectors are defined in FOAF ontology and we will reuse the definition provided in FOAF Agent/Author classes.</rdfs:comment>
        <rdfs:label xml:lang="en">Author</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#CulturalQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#CulturalQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Cultural Question is a Question which asks for a belief of societies, procedures on how to make or prepare things, how to play games, contents of cultural songs, poems used for celebrations etc.</rdfs:comment>
        <rdfs:label xml:lang="en">Cultural Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#DescriptionQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#DescriptionQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Description Question is a Question which asks for a written representation of a given entity. An example of description question could be “What would be the function of X?”.</rdfs:comment>
        <rdfs:label xml:lang="en">Description Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#DichotomousQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#DichotomousQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Dichotomous Question is a Question which asks for a selection of answers from a binary option. It includes Yes/No, Agree/disagree, etc. types of answers for stated questions.</rdfs:comment>
        <rdfs:label xml:lang="en">Dichotomous Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#IllustrationQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#IllustrationQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Illustration Question is a Question which asks for a pictorial, diagrammatic, etc.  representation of a given entity. An example of illustration question could be “What Does X look like?”.</rdfs:comment>
        <rdfs:label xml:lang="en">Illustration Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#MetaphoricalQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#MetaphoricalQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Metaphorical Question is a Question which asks for some conveyed meanings of a given word or expression. Metaphorical questions are related to semasiolgical questions, but they ask for additional interpretation of the expression beyond its obvious meaning.</rdfs:comment>
        <rdfs:label xml:lang="en">Metaphorical Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#MorphologicalQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#MorphologicalQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Morphological Question is a Question which asks about the structure and the formation of words and parts of words. Based on the structure, morphological questions can take various forms.</rdfs:comment>
        <rdfs:label xml:lang="en">Morphological Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#MultipleChoiceQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#MultipleChoiceQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Multiple choice Question is a Question which asks for a selection of one item from a list of three or more potential answers.</rdfs:comment>
        <rdfs:label xml:lang="en">Multiple Choice Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#OnomasiologicalQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#OnomasiologicalQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Onomasiological Question is a Question which asks for the name of a given entity. Onomasiological question may take a form of “how do you call x?”.</rdfs:comment>
        <rdfs:label xml:lang="en">Onomasiological Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#PhonologicalQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#PhonologicalQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Phonological Question is a Question which asks for the pronunciation (Sound), or phonetic representation of words.</rdfs:comment>
        <rdfs:label xml:lang="en">Phonological Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#Question -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#Question">
        <rdfs:comment xml:lang="en">A Question represents an expression used to request information.</rdfs:comment>
        <rdfs:label xml:lang="de">Frage</rdfs:label>
        <rdfs:label xml:lang="en">Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#Questionnaire -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#Questionnaire">
        <rdfs:comment xml:lang="en">A questionnaire represents a set of questions that are related to each other. A questionnaire contains metadata such as questionnaire identifiers, titles, agents and publication related information.  There are six different types of questionnaire which are treated as the subclasses of Questionnaire class.</rdfs:comment>
        <rdfs:label xml:lang="de">Fragebogen</rdfs:label>
        <rdfs:label xml:lang="en">Questionnaire</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#RankingQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#RankingQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Ranking Question is a Question which asks to compare entities and rank them in a certain order.</rdfs:comment>
        <rdfs:label xml:lang="en">Ranking Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#RatingQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#RatingQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Rating Question is a Question which asks the respondent to assign a rate (degree of excellence) to a given entity based on a predefined range.</rdfs:comment>
        <rdfs:label xml:lang="en">Rating Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#SemasiologicalQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#SemasiologicalQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Semasiological Question is a Question which asks for the meaning of a given entity. Semasiological question may take a form of “what does x means?”.</rdfs:comment>
        <rdfs:label xml:lang="en">Semasiological Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#SyntacticQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#SyntacticQuestion">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Question"/>
        <rdfs:comment xml:lang="en">Syntactic Question is a Question which asks for a construction of phrases or sentences using a given word or a given idiom. They have the form “Provide a phrase/sentence for/using a word/idiom X”.</rdfs:comment>
        <rdfs:label xml:lang="en">Syntactic Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#SystematicQuestionnaire -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#SystematicQuestionnaire">
        <rdfs:subClassOf rdf:resource="http://www.localhost:3030/OLDCA#Questionnaire"/>
        <rdfs:comment xml:lang="en">Systematic Questionnaire is a Questionnaire that is used to collect data. This type of questionnaire is used from the beginning of the data collection process.</rdfs:comment>
        <rdfs:label xml:lang="en">Systematic Questionnaire</rdfs:label>
    </owl:Class>
    


    <!-- http://www.localhost:3030/OLDCA#ThesaurusQuestion -->

    <owl:Class rdf:about="http://www.localhost:3030/OLDCA#ThesaurusQuestion">
